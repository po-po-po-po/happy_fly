page {
    background: #eee;
    padding: 20rpx 2.5%;
    width: 95%;
    padding-bottom: 130rpx;
}

.mis-top {
    width: auto;
    height: auto;
    background: #fff;
    padding: 30rpx;
    border-radius: 15rpx;
}

.mis-title {
    width: auto;
    height: auto;
    position: relative;
}

.mis-title image {
    display: block;
    width: 60rpx;
    height: 60rpx;
    float: left;
    border-radius: 10rpx;
    position: absolute;
    left: 0;
    top: 50%;
    margin-top: -30rpx;
}

.mis-title text {
    display: block;
    padding-left: 80rpx;
    font-size: 36rpx;
    line-height: 50rpx;
    color: #333;
    font-weight: bold;
    height: auto;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    float: left;
    width: auto;
}

.mis-cen {
    padding: 15rpx 0;
}

.mic-left {
    display: inline-block;
    width: auto;
    height: 60rpx;
    background: #f2f2f2;
    font-size: 28rpx;
    color: #666;
    text-align: center;
    line-height: 60rpx;
    border-radius: 10rpx;
    padding: 0 15rpx;
}



.mic-tou {
    display: block;
